rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /inquiries/{doc} {
      allow create: if true;
      allow read, update, delete: if request.auth != null;
    }
    match /volunteers/{doc} {
      allow create: if true;
      allow read, update, delete: if request.auth != null;
    }
  }
}